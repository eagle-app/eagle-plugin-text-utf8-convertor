{
    "manifest": {
        "app": {
            "name": "テキストUTF8コンバーター"
        }
    },
    "header": {
        "thumbtack": {
            "isAlwaysOnTop": "常にトップに固定",
            "isNotAlwaysOnTop": "トップに固定解除"
        },
        "dialog": {
            "exit": {
                "title": "プラグインを終了しますか？",
                "description": "終了後、タスクは停止します。未完成のトランスコーディングテキストファイルはもはや変換されません。",
                "cancel": "キャンセル",
                "ok": "終了を確認"
            }
        }
    },
    "main": {
        "table": {
            "thead": {
                "name": "ファイル名",
                "ext": "形式",
                "encoding": "元のコード",
                "content": "元の内容",
                "convertedContent": "エンコード後の内容"
            },
            "contextMenu": {
                "deleteSuccessful": "削除済み"
            }
        },
        "empty": {
            "title": "テキストファイルを追加",
            "content": "Eagleでテキストファイルを追加してから、変換を行ってください。"
        },
        "process": {
            "completed": "変換が完了しました。成功: {{success}}、失敗: {{failed}}。Eagleで他のテキストファイルを選択して続行できます。"
        },
        "dialog": {
            "success": {
                "title": "変換完了！",
                "description": "<span style='color: var(--color-positive)'>{{ count }}</span> 個のテキストファイルを変換しました。Eagleで他のテキストファイルを選択して変換を続けることができます。",
                "cancel": "閉じる",
                "ok": "確認"
            }
        }
    },
    "footer": {
        "convert": {
            "isConverting": "変換中...",
            "title": "エンコードの変更"
        }
    },
    "component": {
        "dropZone": {
            "tip": "ファイルをここにドロップして追加"
        }
    },
    "error": {
        "equalExtension": "拡張子が同じ",

        "fileExtensionNotSupported": "サポートされていないテキストファイル形式"
    }
}
