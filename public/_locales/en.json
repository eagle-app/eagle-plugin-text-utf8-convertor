{
    "manifest": {
        "app": {
            "name": "Text UTF8 Convertor"
        }
    },
    "header": {
        "thumbtack": {
            "isAlwaysOnTop": "Pin Window",
            "isNotAlwaysOnTop": "Unpin"
        },
        "dialog": {
            "exit": {
                "title": "Exit Plugin?",
                "description": "After exiting, the task will stop. Unfinished transcoding text files will no longer be converted.",
                "cancel": "Cancel",
                "ok": "Confirm Exit"
            }
        }
    },
    "main": {
        "table": {
            "thead": {
                "name": "File Name",
                "ext": "Format",
                "encoding": "Original encoding",
                "content": "Original content",
                "convertedContent": "Content after transcoding"
            },
            "contextMenu": {
                "deleteSuccessful": "Deletion Successful"
            }
        },
        "empty": {
            "title": "Add Text File",
            "content": "Please first add a text file from Eagle, then proceed with transcoding."
        },
        "process": {
            "completed": "Conversion completed, success: {{success}}, failed: {{failed}}. You can choose another text file in Eagle to continue transcoding."
        },
        "dialog": {
            "success": {
                "title": "Transcoding completed!",
                "description": "Converted <span style='color: var(--color-positive)'>{{ count }}</span> text files. You can select other text files in Eagle to continue the conversion.",
                "cancel": "Close",
                "ok": "Confirm"
            }
        }
    },
    "footer": {
        "convert": {
            "isConverting": "Transcoding...",
            "title": "Change encoding"
        }
    },
    "component": {
        "dropZone": {
            "tip": "Drag and drop files here to add"
        }
    },
    "error": {
        "equalExtension": "Same File Extension",

        "fileExtensionNotSupported": "Unsupported text file format"
    }
}
