{
    "id": "text-utf8-convertor",
    "version": "1.0.1",
    "platform": "all",
    "arch": "all",
    "name": "{{manifest.app.name}}",
    "logo": "./logo.png",
    "keywords": ["compressor", "image"],
    "fallbackLanguage": "en",
    "languages": ["en", "zh_TW", "zh_CN", "ja_JP", "ko_KR", "es_ES", "de_DE", "ru_RU"],
    "main": {
        "frame": false,
        "vibrancy": true,
        "url": "index.html",
        "width": 960,
        "height": 640,
        "minWidth": 800,
        "minHeight": 600,
        "opacity": 1
    }
}
